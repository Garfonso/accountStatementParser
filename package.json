{
  "name": "accountStatementParser",
  "version": "0.0.1",
  "description": "A simple account statement parser",
  "keywords": [
    "account",
    "statement"
  ],
  "author": {
    "name": "Achim KÃ¶nigs"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Garfonso/accountStatementParser"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && tsc",
    "clean": "node -e \"var { rmdirSync, existsSync } = require('fs'), path = require('path'); ['./.nyc_output', './coverage', './dist'].forEach(fPath => {if (existsSync(path.join(__dirname, fPath))) rmdirSync(path.join(__dirname, fPath), { recursive: true })}); process.exit(0);\"",
    "clean:all": "npm run clean && (rm -r ./node_modules || true)",
    "lint": "eslint --fix src/**/*.ts test/**/*.ts",
    "prepare": "npm run build",
    "release": "release-script",
    "serve": "nodemon ./src/index.ts",
    "test": "mocha",
    "upgrade": "npx npm-check -u"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.8.0",
    "@alcalzone/release-script-plugin-license": "^3.7.0",
    "@eslint/js": "^9.19.0",
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.6 < 23",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.16.0",
    "chai": "^5.1.2",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.9",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.22.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "pdf-parse": "^1.1.1",
    "sitka": "^1.1.1"
  }
}
